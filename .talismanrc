fileignoreconfig:

- filename: defaults/main.yml  
  allowed_patterns:
  - keycloak

- filename: tasks/backup.yml
  ignore_detectors: [filename]

- filename: templates/ansible_keycloak_backup.sh.j2
  ignore_detectors: [filename]
  allowed_patterns:
  - PGPASSWORD=\{\{ \w+ \}\}
  - keycloak
  
- filename: README.md
  allowed_patterns:
  - keycloak